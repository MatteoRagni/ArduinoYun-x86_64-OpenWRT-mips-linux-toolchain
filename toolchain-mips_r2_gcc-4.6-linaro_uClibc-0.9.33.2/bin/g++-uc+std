#!/usr/bin/env bash


WRAPPER_LIBDIR="${WRAPPER_LIBDIR:=-L/home/nirvana1289/data/workspace/toolchain/linino/trunk/staging_dir/target-mips_r2_uClibc-0.9.33.2/usr/lib/}"

WRAPPER_OPTIONS=""
WRAPPER_LIBS="-lc -L/home/nirvana1289/data/workspace/toolchain/linino/trunk/staging_dir/toolchain-mips_r2_gcc-4.6-linaro_uClibc-0.9.33.2/lib -lgcc_s "

WRAPPER_INCLIB="Y"
while [ -n "$1" ]
do
	WRAPPER_OPTIONS="$WRAPPER_OPTIONS $1"
	if [ "$1" = "-c" -o "$1" = "-E" -o "$1" = "-S" -o "$1" = "-MF" ]
	then
		WRAPPER_INCLIB="N"
	fi
	if [ "$1" = "-static" ]
	then
		WRAPPER_LIBS="-lc -L/home/nirvana1289/data/workspace/toolchain/linino/trunk/staging_dir/toolchain-mips_r2_gcc-4.6-linaro_uClibc-0.9.33.2/lib -lgcc_s -lgcc_eh"
	fi
	shift
done
if [ "$WRAPPER_INCLIB" = "Y" ]
then
	WRAPPER_OPTIONS="$WRAPPER_OPTIONS -nodefaultlibs $WRAPPER_LIBDIR -Wl,-Bdynamic,-luClibc++,-Bstatic,-lstdc++,-Bdynamic $WRAPPER_LIBS"
fi

exec mips-openwrt-linux-uclibc-g++ -fno-builtin  -DGCC_HASCLASSVISIBILITY  $WRAPPER_INCLUDEDIR $WRAPPER_OPTIONS
